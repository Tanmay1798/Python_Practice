   "source": [
    "# 1. Write a python program to calculate the sum of a list of number"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "3ea4d70b",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Original List =  [1, 2, 3, 3, 4, 5]\n",
      "Sum of list =  18\n"
     ]
    }
   ],
   "source": [
    "def sum(l):\n",
    "    total = 0\n",
    "    for i in range(len(l)):\n",
    "        total = total + l[i]\n",
    "    return total\n",
    "\n",
    "lt = [1 , 2 , 3 , 3 , 4 , 5]\n",
    "print(\"Original List = \",lt)\n",
    "print(\"Sum of list = \" , sum(lt))"
   ]
